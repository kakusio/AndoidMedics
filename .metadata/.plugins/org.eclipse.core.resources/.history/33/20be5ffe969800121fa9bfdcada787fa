package com.example.nfcandroid;

import java.util.ArrayList;

public class EditableInformationPersona {
	public String Name;
	public String Value;
	public int Id;
	
	public EditableInformationPersona(String Name, String Value, int Id) {
		this.Name = Name;
		this.Value = Value;
		this.Id = 200200 + Id;
	}

	public static ArrayList<EditableInformationPersona> GetEditableInformation(){
		ArrayList<EditableInformationPersona> editable_information= new ArrayList<EditableInformationPersona>();
		editable_information.add(new EditableInformationPersona("Email","", 1));
		editable_information.add(new EditableInformationPersona("TelefonoResidencial","", 2));
		editable_information.add(new EditableInformationPersona("TelefonoCelular","", 3));
		editable_information.add(new EditableInformationPersona("Direccion","", 4));
		return editable_information;
	}
}
