package com.example.nfcandroid;

import java.util.ArrayList;

import org.json.JSONException;
import org.json.JSONObject;

public class EditableInformationPersona {
	public String Name;
	public String Value;
	
	public EditableInformationPersona(String Name, String Value) {
		this.Name = Name;
		this.Value = Value;
	}

	public static ArrayList<EditableInformationPersona> GetEditableInformation(){
		ArrayList<EditableInformationPersona> editable_information= new ArrayList<EditableInformationPersona>();
		editable_information.add(new EditableInformationPersona("Email",""));
		editable_information.add(new EditableInformationPersona("TelefonoResidencial",""));
		editable_information.add(new EditableInformationPersona("TelefonoCelular",""));
		editable_information.add(new EditableInformationPersona("Direccion",""));
		return editable_information;
	}
}
